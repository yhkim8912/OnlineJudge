#include <iostream>
#include <cstdio>

using namespace std;

int main() {
	
	int line = 3;
	int a, b, c, d;
	char result;

	while (line--) {
		scanf("%d %d %d %d", &a, &b, &c, &d);

		switch (a + b + c + d) {
		case 0:
			result = 'D';
			break;
		case 1:
			result = 'C';
			break;
		case 2:
			result = 'B';
			break;
		case 3:
			result = 'A';
			break;
		case 4:
			result = 'E';
			break;
		default:
			break;
		}
		cout << result << endl;
	}

	return 0;
}
